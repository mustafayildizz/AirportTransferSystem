{% extends 'base.html.twig' %}

{% block title %}Car{% endblock %}

{% block body %}
    <h1>Car</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ car.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ car.title }}</td>
            </tr>
            <tr>
                <th>Companyid</th>
                <td>{{ car.companyid }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ car.description }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ car.image }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ car.price }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ car.status }}</td>
            </tr>
            <tr>
                <th>Number</th>
                <td>{{ car.number }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_car_index') }}">back to list</a>

    <a href="{{ path('admin_car_edit', {'id': car.id}) }}">edit</a>

    {{ include('admin/car/_delete_form.html.twig') }}
